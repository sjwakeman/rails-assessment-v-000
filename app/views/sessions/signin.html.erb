<h2>Sign In Page</h2>

<%= form_for @user, url: signin_path(@user) do |f| %> 
<table>
<tr><td> </td></tr>
  <tr><td><strong><%= f.label :name %> :</strong>
  <%= f.text_field :name %></td></tr>
  <tr><td> </td></tr>
  <tr><td> </td></tr>
  <tr><td><strong><%= f.label :password %> :</strong>
  <%= f.text_field :password %></td></tr>
  <tr><td> </td></tr>
  <tr><td> </td></tr>
  <tr><td><%= f.submit 'Log In'%></td></tr><!--User/show-->
</table>
<% end %>
<table>
  <tr><td> New user? <%= link_to "Sign up now!", new_user_path %></td></tr><!--User/new-->
</table>

