<%= form_for @client do |f| %>
 <%= f.text_field :client_name, list: "client_name_autocomplete" %>
 <datalist id="client_name_autocomplete">
   <% Client.all.each do |client| %>
     <option value="<%= client.name %>">
   <% end %>
 </datalist>
 <textarea name="client[name]"></textarea>
<% end %>

