<%= form_for @training_sessions do |f| %>
  <div class=”field”>
    <%= f.label :date %> :
    <%= f.text_field :date %><br><br>
  </div>
  <div class=”field”>
    <%= f.label :start_time %> :
    <%= f.text_field :start_time %><br><br><br>
  </div>
  <div class=”field”>
    <%= f.label :end_time %> :
    <%= f.text_field :end_time %><br><br>
  </div>
  <div class=”field”>
    <%= f.label :location %> :
    <%= f.text_field :location %><br><br>
  </div>
  <div class=”actions”>
    <%= f.submit %>
  </div>
<% end %>
