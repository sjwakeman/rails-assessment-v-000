<%= form_for @training_sessions.new do |f| %>
  <div class=”field”>
    <%= f.label :date %> :
    <%= f.text_field :date %>

  </div>

  <div class=”field”>
    <%= f.label :start_time %> :
    <%= f.text_field :start_time %>

  </div>
  <div class=”field”>
    <%= f.label :end_time %> :
    <%= f.text_field :end_time %>

  </div>
  <div class=”field”>
    <%= f.label :location %> :
    <%= f.text_field :location %>

  </div>
  <div class=”field”>
    <%= f.label :booked_status %> :
    <%= f.text_field :booked_status %>

  </div>
  <div class=”actions”>
    <%= f.submit %>
  </div>
<% end %>
