<%= form_for @training_session.new do |f| %>
  <table>
    <tr><td><%= f.label :date %> :
    <%= f.text_field :date %></td></tr>
    <tr><td> </td></tr>

    <tr><td><%= f.label :client %> :
    <%= f.text_field :client %>
    <%= f.collection_select(:client, Client.all, :id, :name) %></td></tr>
    <tr><td> </td></tr>

    <tr><td><%= f.label :start_time %> :
    <%= f.text_field :start_time %></td></tr>
    <tr><td> </td></tr>
    <tr><td><%= f.label :end_time %> :
    <%= f.text_field :end_time %></td></tr>
    <tr><td> </td></tr>

    <tr><td><%= f.label :location %> :
    <%= f.text_field :location %></td></tr>
    <tr><td> </td></tr>

    <tr><td><%= f.label :booked_status %> :
    <%= f.text_field :booked_status %></td></tr>
    <tr><td> </td></tr>

    <tr><td><%= f.submit %></td></tr>
    <tr><td> </td></tr>
  </table>
<% end %>
